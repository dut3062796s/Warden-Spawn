        },
        {
          "type": "onCompletedAsync",
          "condition": "invalidCheckResult",
          "action": "sendGridIntegration",
          "configuration": {
            "apiKey": "ABC",
            "sender": "test@email.com"
          }
        }
      ]
    }
  ]
}